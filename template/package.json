    "ios:dev": "npx react-native run-ios --scheme=dev",
    "ios:local": "npx react-native run-ios --scheme=local",
    "ios:clear": "chmod +x ./dev-tools/ios_clear.sh && ./dev-tools/ios_clear.sh",
